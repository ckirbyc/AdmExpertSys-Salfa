                                        contentido += '<tr class="trGrupo' + i + '">';
                                        contentido += '<td>' + (data.Grupo[i].NumeroGrupo) + '</td>';
                                        contentido += '<td>' + (data.Grupo[i].NombreGrupo == null ? '' : data.Grupo[i].NombreGrupo) + '</td>';
                                        contentido += '<td>' + (data.Grupo[i].CorreoGrupo == null ? '' : data.Grupo[i].CorreoGrupo) + '</td>';
                                        contentido += '<td class="columnaAcciones textCenter">';
                                        contentido += '<a style="cursor: pointer;" class="glyphicon glyphicon-remove colorOrange" onclick="eliminarGrupo(' + i + ', ' + nomGrupo + ',1)"></a> <text>|</text> ';
                                        contentido += '</td></tr>';
                                        nomGrupoSel = data.Grupo[i].NombreGrupo + ',' + nomGrupoSel;
                                    }
                                }
                                $('#tbGrupos tbody').html(contentido);
                                $("#NombreGrupoSel").val(nomGrupoSel);
                                if (nomGrupoSel != '') {
                                    $("#btnGuardarGrupo").show();
                                }
                            } else {
                                BootstrapDialog.alert({
                                    title: '¡Error al buscar Grupo en el AD!',
                                    message: '<strong>No existe Grupo</strong>',
                                    type: BootstrapDialog.TYPE_DANGER
                                });
                            }
                        }
                    },
                    complete: function () {
                        $("#NombreGrupo").val('');
                        $("#loading-modal").hide();
                    },
                    error: function (data) {
                        $("#loading-modal").hide();
                        BootstrapDialog.alert({
                            title: '¡Error al buscar Grupo en el AD!',
                            message: '<strong>' + data.Error + '</strong>',
                            type: BootstrapDialog.TYPE_DANGER
                        });
                    }
                });
            }

            $("#btnGuardarGrupo").off('click').click(function () {
                $("#loading-modal").show();
                var valida = true;
                var userName = $("#NombreUsuario").val();
                var selGrupos = $("#NombreGrupoSel").val();
                if (userName == '') {
                    valida = false;
                    $("#loading-modal").hide();
                    BootstrapDialog.alert({
                        title: '¡Advertencia!',
                        message: '<strong>Debe asignar un ID de Usuario</strong>',
                        type: BootstrapDialog.TYPE_WARNING
                    });
                }
                if (selGrupos == '') {
                    valida = false;
                    $("#loading-modal").hide();
                    BootstrapDialog.alert({
                        title: '¡Advertencia!',
                        message: '<strong>Debe buscar y asignar Grupo</strong>',
                        type: BootstrapDialog.TYPE_WARNING
                    });
                }

                if (valida == true) {
                    setTimeout(function () {
                        guardarGrupos();
                    }, 1000);
                }
            });

            function guardarGrupos() {
                $.ajax({
                    url: '@Url.Action("GuardarGrupos", "Home")',
                    type: 'POST',
                    contentType: "application/json; charset=utf-8",
                    async: false,
                    dataType: 'json',
                    processData: false,
                    data: JSON.stringify({ nomGrupos: $("#NombreGrupoSel").val(), userName: $("#NombreUsuario").val(), upnPrefijo: $("#UpnPrefijo").val() }),
                    success: function (data) {
                        if (data.Session == false) {
                            BootstrapDialog.alert({
			                        title: '¡Validación Sesión!',
			                        message: '<strong>Su sesión ha expirado. Seleccione <a href="@Url.Action("IndexLogin", "Error", new { mensajeError = "Su sesión ha expirado." })">aquí</a> para iniciar sesión nuevamente</strong>',
			                        type: BootstrapDialog.TYPE_WARNING
		                        });
                            return;
                        }

                        if (data.Error != '') {
                            BootstrapDialog.alert({
                                title: '¡Error al guardar Grupo en el AD!',
                                message: '<strong>' + data.Error + '</strong>',
                                type: BootstrapDialog.TYPE_DANGER
                            });
                        } else {
                            if (data.Validar) {
                                if (data.TieneLicencia) {
                                    BootstrapDialog.alert({
                                        title: '¡Proceso Exitoso!',
                                        message: '<strong>Grupos guardados exitosamente y licencia se encuentra asignada a la cuenta.</strong>',
                                        type: BootstrapDialog.TYPE_SUCCESS
                                    });
                                } else {
                                    BootstrapDialog.alert({
                                        title: '¡Proceso Incompleto!',
                                        message: '<strong>Grupos guardados exitosamente, pero debe asignar licencia a la cuenta para terminar proceso.</strong>',
                                        type: BootstrapDialog.TYPE_WARNING
                                    });
                                }
                            } else {
                                BootstrapDialog.alert({
                                    title: '¡Error al guardar Grupo en el AD!',
                                    message: '<strong>No se pudo asignar usuario a los Grupos, intente más tarde.</strong>',
                                    type: BootstrapDialog.TYPE_DANGER
                                });
                            }
                        }
                    },
                    complete: function () {
                        $("#loading-modal").hide();
                    },
                    error: function (data) {
                        $("#loading-modal").hide();
                        BootstrapDialog.alert({
                            title: '¡Error al guardar Grupo en el AD!',
                            message: '<strong>' + data.Error + '</strong>',
                            type: BootstrapDialog.TYPE_DANGER
                        });
                    }
                });
            }

            $('#Rut').on('blur', function () {
                var rut = $("#Rut").val();
                if (rutValido(rut)) {
                    formateaRut(this, 'XXXXXXX-X');
                }
                else {
                    BootstrapDialog.show({
                        title: '¡Validación!',
                        message: '<strong>Rut Incorrecto</strong>',
                        type: BootstrapDialog.TYPE_DANGER,
                        buttons: [{
                            label: 'Aceptar',
                            cssClass: 'btn-default btn-sm',
                            action: function (dialog) {
                                dialog.close();
                            }
                        }]
                    });
                    $('#Rut').focus();
                    $("#Rut").val("");
                }
            });
        });
</script>
<script language="javascript" type="text/javascript">
        function eliminarGrupo(idTrGrupo, nombreGrupoElm, tipo) {
            var nomGrupoSel = $("#NombreGrupoSel").val();
            var nomGrupoSelFinal = '';
            if (nomGrupoSel != '') {
                var nomGrupoSplit = $("#NombreGrupoSel").val().split(',');
                $("#NombreGrupoSel").val('');
                for (var i = 0; i < nomGrupoSplit.length; i++) {
                    var nomGrupoIngresado = nomGrupoSplit[i];
                    if (nomGrupoIngresado.trim() != nombreGrupoElm.trim() && nomGrupoIngresado.trim() != '') {
                        nomGrupoSelFinal = nomGrupoIngresado.trim() + ',' + nomGrupoSelFinal;
                    }
                }
            }

            $("#NombreGrupoSel").val(nomGrupoSelFinal);
            if(tipo == 1)
                $('.trGrupo' + idTrGrupo).html('');
            else
                $('.trGrupoOu' + idTrGrupo).html('');

            if (nomGrupoSelFinal == '' && tipo == 1) {
                $("#btnGuardarGrupo").hide();
            }
        }

        function asignarGrupoUsuario(idTrGrupoAsig, nombreGrupoAsig)
        {
            MostrarCargando();
            var valida = true;
